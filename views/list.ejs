<div class="container-fluid px-4">
    <h4 class="mt-2"></h4>
    <div class="row">
        <div class="col-12">
            <div class="card mb-4">
                <div class="card-header">
                    <i class="fas fa-chart-bar me-1"></i>
                    생산 내역
                </div>
                <div class="card-body dashboard-card-body">
                    <div>
                        <button type="button" class="btn btn-light">오늘</button>
                        <button type="button" class="btn btn-light">이번주</button>
                        <button type="button" class="btn btn-light">이번달</button>
                        <input type="text" id="datePicker" class="form-control">
                    </div>
                    <table id="production_list_table" class="display nowrap" style="width:100%">
                        <thead>
                            <tr>
                                <th>모듈 번호</th>
                                <th>모듈 이름</th>
                                <th>용접 정보</th>
                                <th>전압 정보</th>
                                <th>생산 일시</th>
                            </tr>
                        </thead>
                        <tbody>
                            <% for(var i=0; i<100; i++){ %>
                                <tr>
                                    <td>가나다-0000<%=i %>
                                    </td>
                                    <% if(i%2==0) {%>
                                        <td>module-A</td>
                                        <%} else{ %>
                                            <td>module-B</td>
                                            <%} %>
                                                <td>36</td>
                                                <td>15</td>
                                                <td>2024-01-17 16:23:15</td>
                                </tr>
                                <%} %>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>

    </div>
</div>

