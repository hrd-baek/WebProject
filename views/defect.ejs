<div class="container-fluid px-4">
    <h4 class="mt-2"></h4>
    <div class="row">
        <div class="col-12">
            <div class="card mb-4">
                <div class="card-header">
                    <i class="fas fa-chart-bar me-1"></i>
                    불량 현황
                </div>
                <div class="card-body dashboard-card-body">
                    <table id="datatablesSimple">
                        <thead>
                            <tr>
                                <th>모듈 번호</th>
                                <th>모듈 이름</th>
                                <th>불량 유형</th>
                                <th>발생 일시</th>
                            </tr>
                        </thead>
                        <tbody>
                            <% for(var i=0; i<100; i++){ %>
                                <tr>
                                    <td>abc-0000<%=i %>

                                    </td>
                                    <% if(i%2==0) {%>
                                        <td>모듈 A</td>
                                        <%} else{ %>
                                            <td>모듈 B</td>
                                            <%} %>
                                            <td>용접 불량</td>
                                                <td>2024-01-17 16:23:15</td>
                                </tr>
                                <%} %>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
        <div class="col-6">

        </div>
    </div>

</div>
<script src="/js/defect.js" defer></script>